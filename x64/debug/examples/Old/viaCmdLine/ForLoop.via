// ForLoop.lnl

application PE GUI;

include "Windows.inc", "Conversion.inc";

dword i;
string buffer;

entry
    for(i=0;i<=6;i++) {
        MessageBox(0,CStr(i),"For Loop..",$20);
    }
    for(i=1;i<=3;i++) {
        Format(buffer,"%d",i);
        MessageBox(0,buffer,"More For Loop..",$20);
    }
end.

